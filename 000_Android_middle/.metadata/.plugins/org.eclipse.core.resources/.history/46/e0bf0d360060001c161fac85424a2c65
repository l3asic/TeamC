<mapper namespace="org.mybatis.example.BlogMapper">

	<select id="abcd" resultType="dto.GridDTO">
		select * from GridDTO
	</select>


<!-- ============안드로이드 과제 ㅋㅋ -->
	<insert id="SignOn">
		insert into and_acc(id, pw, name, birth)
		values (#{id}, #{pw}, #{name}, #{birth})
	</insert>
	
	<select  id="isAccounted">
		select * from and_acc where id = #{id}
	</select>
	
	<select  id="isSignIn">
		select * from and_acc where id = #{id} and pw=#{pw}
	</select>
<!-- ============안드로이드 과제 ㅋㅋ -->

</mapper>